{
  "name": "codethreads",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "go": "git add . && aicommits && git push origin main",
    "lint": "next lint",
    "db:push": "supabase db push && pnpm db:types",
    "db:diff": "supabase db diff -f migration",
    "db:types": "supabase gen types typescript > lib/supabase/database.types.ts",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:status": "supabase status",
    "db:local:push": "supabase db push --local",
    "db:local:types": "supabase gen types typescript --local > lib/supabase/database.types.ts",
    "db:local:reset": "supabase db reset --local && supabase db push --local",
    "db:local:new": "supabase migration new",
    "db:local:list": "supabase migration list --local",
    "db:local:status": "supabase status --local",
    "db:reset": "docker volume rm supabase_db_codethreads || true && pnpm db:stop && pnpm db:start",
    "db:restart": "pnpm db:stop && pnpm db:start",
    "db:clean": "docker volume rm supabase_db_codethreads || true",
    "db:logs": "supabase logs",
    "db:migrate": "supabase db diff -f",
    "db:deploy": "pnpm db:reset && pnpm db:push",
    "db:remote:push": "supabase db push",
    "db:check": "supabase db lint"
  },
